<h1>Login</h1>

<% form_for @user_session do |f| %>
	<%= f.error_messages %>
	<fieldset>
		<div class="formFields">
			<%= f.label :username %>
			<%= f.text_field :username %>
		</div>
		<div class="formFields">
			<%= f.label :password %>
			<%= f.password_field :password %>
		</div>
	</fieldset>
	<%= f.submit "Submit" %>
<% end %>